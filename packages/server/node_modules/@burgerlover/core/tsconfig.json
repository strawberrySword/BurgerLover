{
	"compilerOptions": {
		"target": "ES5",
		"module": "commonjs",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"rootDir": "src",
		"outDir": "dist",
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true
	},
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Recommended"
}
